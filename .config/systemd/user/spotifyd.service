[Unit]
Description=A spotify playing daemon
Documentation=https://github.com/Spotifyd/spotifyd
Wants=sound.target
After=sound.target
Wants=network-online.target
After=network-online.target

[Service]
ExecStart=/home/cewillis3/.cargo/bin/spotifyd --no-daemon --backend pulseaudio --cache-path /home/cewillis3/.cache/spotifyd
Restart=always
RestartSec=12

[Install]
WantedBy=default.target
